{
   "name": "frdlweb/server",
  "description": "Root-Project for a server/webspace/vhost with backend, shares, and subprojects.",
  "keywords": ["webfan", "webspace", "server", "subscription", "vhost", "root", "php"],
  "type" : "webfan-server",
  "license": ["MIT"],	  
  "homepage": "https://frdlweb.de",	
  "repositories": [	
       {"type": "composer", "url": "https://repo.pkg.frdl.de"},
       {"packagist": false}
  ],
 "authors": [{
		"name": "Till Wehowski",
		"email": "software@frdl.de",
		"homepage": "https://frdl.de"
  }],
  "require" : {	
    "php": ">=7.0 || >=8.0",	
    "frdlweb/frdlweb-composer-plugin": "*"
  },
  "require-dev": {
     	"roave/security-advisories": "dev-latest",
    	"yosymfony/resource-watcher": "*"
  },	
   "minimum-stability": "dev",
   "extra": { 
     "io4" : {   
	  "tenant-allow-packages" : {
	     "frdl/1.3.6.1.4.1.37553.8.2.49363662052.3.1276945-io4-rdap" : true,
             "frdlweb/webfat" : {
                "allow" : true,
	        "scope" : "@cwd"
	     }
	 },
	 "auto-install-packages" : {
	     "frdl/1.3.6.1.4.1.37553.8.2.49363662052.3.1276945-io4-rdap" : "*",
             "frdlweb/webfat" : {
		     "on" : [
			     "circuit:1.3.6.1.4.1.37553.8.1.8.8.1958965301.5.1" : {               
				     "allow" : true,	     
				     "scope" : "@www@parent"	   
			     }, 			  
			     "tenancy" : {             
				     "allow" : true,	      
				     "scope" : "@cwd"	  
			     },			  
			     "1.3.6.1.4.1.37553.8.1.8.1.1089085" : {             
				     "allow" : true,	       
				     "scope" : "@www@parent"	   
			     }
		     ]
	     },
	     "@test" : {
		     "install" : true,
		     "onBuildFlavor" : ["1.3.6.1.4.1.37553.8.1.8.1.575874"]
	     }
	 }
     },
    "webfan" : {  
	"registration" : {
              "installed" : false
	},
	"api" : {
               "installed" : false
	},
       "webfat" : {
	  "scope" : "@www@root",
	  "FRDL_BUILD_FLAVOR" : "1.3.6.1.4.1.37553.8.1.8.1.575874",
          "1.3.6.1.4.1.37553.8.1.8.1.1089085" : {
              "description" : "Webmaster FRDL_BUILD_FLAVOR",
	      "enabled" : true,
	      "modules" : []
	  },
          "1.3.6.1.4.1.37553.8.1.8.1.575874" : {
              "description" : "Developer FRDL_BUILD_FLAVOR",
	      "enabled" : true,
	      "modules" : []
	  }
       }
   }
  }
}
